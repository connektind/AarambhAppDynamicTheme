<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white"
    tools:context=".activity.ProfileShowActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="180dp"
        android:orientation="vertical"
        android:id="@+id/profile_background"
        android:background="@drawable/red_gradient">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="10dp"
            android:gravity="center_vertical">

            <ImageButton
                android:layout_width="35dp"
                android:background="@drawable/ic_back"
                android:backgroundTint="@color/white"
                android:id="@+id/back_btn_profile_show"
                android:layout_height="35dp"/>

            <ImageButton
                android:layout_width="35dp"
                android:background="@drawable/edit_profile"
                android:layout_alignParentRight="true"
                android:id="@+id/edit_profile_red"
                android:layout_height="35dp"/>

        </RelativeLayout>

        <TextView
            android:layout_width="wrap_content"
            android:text=""
            android:textColor="@color/white"
            android:textSize="20sp"
            android:layout_marginTop="15dp"
            android:layout_centerInParent="true"
            android:textStyle="bold"
            android:id="@+id/student_name_red_profile_view"
            android:fontFamily="@font/futu_md"
            android:layout_height="wrap_content"/>

        <TextView
            android:layout_width="wrap_content"
            android:text="6th Grade"
            android:layout_below="@id/student_name_red_profile_view"
            android:textColor="@color/white"
            android:textSize="16dp"
            android:layout_centerInParent="true"
            android:fontFamily="@font/futu_md"
            android:layout_height="wrap_content"/>

    </RelativeLayout>

    <LinearLayout
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="-50dp"
        android:layout_gravity="center"
        android:gravity="center"
        android:padding="3dp"
        android:background="@drawable/practice_bg"
        android:backgroundTint="@color/white"
        android:orientation="vertical">
        <ImageView
            android:id="@+id/img_profile_1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:srcCompat="@drawable/circle_back" />
    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingStart="15dp"
                android:paddingEnd="15dp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:gravity="center_vertical"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:gravity="center"
                        android:padding="10dp"
                        android:background="@drawable/circle_back">

                        <ImageView
                            android:layout_width="match_parent"
                            android:background="@drawable/mobile_profile"
                            android:id="@+id/mobile_profile_image"
                            android:layout_height="match_parent"/>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="20dp"
                        android:orientation="vertical">
                        <TextView
                            android:layout_width="match_parent"
                            android:text="Mobile Number"
                            android:layout_height="wrap_content"/>
                        <TextView
                            android:layout_width="match_parent"
                            android:text=""
                            android:textColor="@color/red"
                            android:textSize="16dp"
                            android:id="@+id/user_mobile_red"
                            android:textStyle="bold"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                android:layout_width="match_parent"
                android:layout_marginTop="7dp"
                android:gravity="center_vertical"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:gravity="center"
                    android:padding="10dp"
                    android:background="@drawable/circle_back">

                    <ImageView
                        android:layout_width="match_parent"
                        android:background="@drawable/name_profile"
                        android:id="@+id/name_profile_image"
                        android:layout_height="match_parent"/>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="20dp"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="match_parent"
                        android:text="Name"
                        android:layout_height="wrap_content"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:text=""
                        android:textColor="@color/red"
                        android:textSize="16dp"
                        android:textStyle="bold"
                        android:id="@+id/student_name_profile_red"
                        android:layout_height="wrap_content"/>
                </LinearLayout>
            </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_marginTop="7dp"
                    android:gravity="center_vertical"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:gravity="center"
                        android:padding="10dp"
                        android:background="@drawable/circle_back">

                        <ImageView
                            android:layout_width="match_parent"
                            android:background="@drawable/email_profile"
                            android:id="@+id/email_profile_image"
                            android:layout_height="match_parent"/>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="20dp"
                        android:orientation="vertical">
                        <TextView
                            android:layout_width="match_parent"
                            android:text="Email Address"
                            android:layout_height="wrap_content"/>
                        <TextView
                            android:layout_width="match_parent"
                            android:text=""
                            android:textColor="@color/red"
                            android:textSize="16dp"
                            android:id="@+id/student_profile_email"
                            android:textStyle="bold"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_marginTop="7dp"
                    android:gravity="center_vertical"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:gravity="center"
                        android:padding="10dp"
                        android:background="@drawable/circle_back">

                        <ImageView
                            android:layout_width="match_parent"
                            android:background="@drawable/gende_profile"
                            android:id="@+id/gender_profile_image"
                            android:layout_height="match_parent"/>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="20dp"
                        android:orientation="vertical">
                        <TextView
                            android:layout_width="match_parent"
                            android:text="Gender"
                            android:layout_height="wrap_content"/>
                        <TextView
                            android:layout_width="match_parent"
                            android:text=""
                            android:textColor="@color/red"
                            android:textSize="16dp"
                            android:id="@+id/student_profile_gender"
                            android:textStyle="bold"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_marginTop="7dp"
                    android:gravity="center_vertical"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:gravity="center"
                        android:padding="10dp"
                        android:background="@drawable/circle_back">

                        <ImageView
                            android:layout_width="match_parent"
                            android:background="@drawable/dob_profile"
                            android:id="@+id/Dob_profile_image"
                            android:layout_height="match_parent"/>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="20dp"
                        android:orientation="vertical">
                        <TextView
                            android:layout_width="match_parent"
                            android:text="Date of Birth"
                            android:layout_height="wrap_content"/>
                        <TextView
                            android:layout_width="match_parent"
                            android:text=""
                            android:textColor="@color/red"
                            android:textSize="16dp"
                            android:id="@+id/student_date_of_birth_profile"
                            android:textStyle="bold"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_marginTop="7dp"
                    android:gravity="center_vertical"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:gravity="center"
                        android:padding="10dp"
                        android:background="@drawable/circle_back">

                        <ImageView
                            android:layout_width="match_parent"
                            android:background="@drawable/home_profile"
                            android:id="@+id/address_profile_image"
                            android:layout_height="match_parent"/>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="20dp"
                        android:orientation="vertical">
                        <TextView
                            android:layout_width="match_parent"
                            android:text="Home Address"
                            android:layout_height="wrap_content"/>
                        <TextView
                            android:layout_width="match_parent"
                            android:text=""
                            android:textColor="@color/red"
                            android:textSize="16dp"
                            android:id="@+id/student_adddress_Profile"
                            android:textStyle="bold"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>

                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:background="@color/hint_color"
                    android:layout_marginLeft="70dp"
                    android:layout_marginTop="10dp"
                    android:layout_height="1dp"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_marginTop="7dp"
                    android:gravity="center_vertical"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:gravity="center"
                        android:padding="10dp"
                        android:background="@drawable/circle_back">

                        <ImageView
                            android:layout_width="match_parent"
                            android:background="@drawable/logout_profile"
                            android:id="@+id/logout_image"

                            android:layout_height="match_parent"/>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="20dp"
                        android:orientation="vertical">
                        <TextView
                            android:layout_width="match_parent"
                            android:text="Logout"
                            android:textColor="@color/red"
                            android:textSize="16dp"
                            android:id="@+id/logout_profile"
                            android:textStyle="bold"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </androidx.core.widget.NestedScrollView>



    </LinearLayout>

</LinearLayout>





